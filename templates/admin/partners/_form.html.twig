{{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
 {{ form_row(form._token) }}
<form id="addPartnerForm" action="" method="post">
    <div class="card-content">
        <div class="form-group">
            <label class="control-label">
                Société
            </label>
            {{ form_row(form.company) }}
        </div>
        <div class="form-group">
            <label class="control-label">
                Prénom <star>*</star>
            </label>
            {{ form_row(form.firstName) }}
        </div>
        <div class="form-group">
            <label class="control-label">
                Nom<star>*</star>
            </label>
            {{ form_row(form.lastName) }}
        </div>
        <div class="form-group">
            <label class="control-label">
                E-Mail
            </label>
            {{ form_row(form.email) }}
        </div>
        <div class="form-group">
            <label class="control-label">
                Site Web
            </label>
            {{ form_row(form.url, {'attr': {'value': 'http://'}}) }}
        </div>
        <div class="form-group">
            <label class="control-label">
                Ville<star>*</star>
            </label>
            {{ form_row(form.city) }}
        </div>
        <div class="form-group">
            <label class="control-label">
                Pays<star>*</star>
            </label>
            {{ form_row(form.country) }}
        </div>
        <div class="category"><star>*</star> Champs obligatoires</div>
        <div class="card-footer">
            <a class="btn btn-danger btn-fill pull-right" href="{{ path('partners_index') }}" style="margin-left: 1rem;">Annuler</a>
            <button class="btn btn-info btn-fill pull-right">{{ button_label|default('Enregistrer') }}</button>
            <div class="clearfix"></div>
        </div>
    </div>
</form>
{{ form_end(form) }}
